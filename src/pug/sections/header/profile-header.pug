//begin profile-header
.profile-header
  .container
    //begin profile-header__search
    form.profile-header__search
      .profile-header__search-submit
        input(type='submit')
        +svg('magnific')
      input(type='search' name='search' placeholder='Поиск').profile-header__search-input
      .profile-header__search-selectbox
        select(name='search-select')
          option(value='all' selected) Все категории
          option(value='1') Категория 1
          option(value='2') Категория 2
          option(value='3') Категория 3
        +svg('arr-down')
    //end profile-header__search

    //begin profile-header__icons-list
    ul.profile-header__icons-list
      li.profile-header__icons-item
        a(href="#popup-enter").profile-header__icons-link.profile-header__icons-link--message.active
          +svg('messages')
          .profile-header__icons-counter
            span 22

        //begin profile-header-popup
        .profile-header-popup-wrap
          .profile-header-popup
            .profile-header-popup__header
              span Новые

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-05.jpg" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text Александра Степанова хочет добавить Вас в друзья
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Добавить
                  button.btn.btn--grey
                    span Отклонить
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-05.jpg" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text Камилла Владимирова отправила Вам сообщение
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Прочитать
            //end profile-header-popup__item

            .profile-header-popup__footer
              a(href="#") Показать еще
        //end profile-header-popup

      li.profile-header__icons-item
        a(href="#popup-form-regist").profile-header__icons-link.profile-header__icons-link--hat
          +svg('hat')

        //begin profile-header-popup
        .profile-header-popup-wrap
          .profile-header-popup
            .profile-header-popup__header
              span Новые

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                +svg('monitor')
              .profile-header-popup__info
                span.profile-header-popup__text 01.08 состоится вебинар Психология 2.0
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-11.png" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text Куратор проверил домашнее задание курса Психология 2.0. Новое Начало
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-11.png" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text У Вас новый комментарий куратора в курсе Психология 2.0
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                +svg('monitor')
              .profile-header-popup__info
                span.profile-header-popup__text 01.08 состоится вебинар Психология 2.0
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            .profile-header-popup__footer
              a(href="#") Показать еще
        //end profile-header-popup

      li.profile-header__icons-item
        a(href="#popup-form").profile-header__icons-link.profile-header__icons-link--info
          +svg('info')

        //begin profile-header-popup
        .profile-header-popup-wrap
          .profile-header-popup
            .profile-header-popup__header
              span Новые

            //begin profile-header-popup__item
            .profile-header-popup__item.profile-header-popup__item--counter
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                span 5
              .profile-header-popup__info
                span.profile-header-popup__text Через 5 дней заканчивается Ваша подписка
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Продлить подписку
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-05.jpg" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text Ваш комментарий к записи блога Александры Степановой удален
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            .profile-header-popup__header.profile-header-popup__header--old
              span Прочитанные

            //begin profile-header-popup__item
            .profile-header-popup__item.profile-header-popup__item--old.profile-header-popup__item--counter
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                span 5
              .profile-header-popup__info
                span.profile-header-popup__text Через 5 дней заканчивается Ваша подписка
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Продлить подписку
            //end profile-header-popup__item

            //begin profile-header-popup__item
            .profile-header-popup__item.profile-header-popup__item--old
              .profile-header-popup__close
                +svg('close')
              .profile-header-popup__ava
                img(src="assets/img/friends/friend-05.jpg" alt="picture")
              .profile-header-popup__info
                span.profile-header-popup__text Ваш комментарий к записи блога Александры Степановой удален
                .profile-header-popup__btns
                  button.btn.btn--dark
                    span Посмотреть
            //end profile-header-popup__item

            .profile-header-popup__footer
              a(href="#") Показать еще
        //end profile-header-popup

    //end profile-header__icons-list

    //begin profile-header__user
    .profile-header__user
      .profile-header__user-ava
        img(src="assets/img/profile-header-ava.jpg" alt="ava")
      .profile-header__user-body
        h3.profile-header__user-name Мария Иванова
        h4.profile-header__user-rait
          span 4 000
          =' '
          |баллов
        .profile-header__user-arrow
          +svg('arr-down')
      ul.profile-header__user-menu#profile-header__user-menu
        li.profile-header__user-item
          a(href="#").profile-header__user-link Моя страница
        li.profile-header__user-item.active
          a(href="#").profile-header__user-link Мой счет: 4000 баллов
        li.profile-header__user-item
          a(href="#").profile-header__user-link Редактировать
        li.profile-header__user-item
          a(href="#").profile-header__user-link Выйти
    //end profile-header__user

//end   profile-header