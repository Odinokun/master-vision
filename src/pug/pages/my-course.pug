extends ../_template

block variables
  - var title       = 'Master Vision'
  - var description = 'Master Vision'
  - var ogtitle     = 'Master Vision'

block content
  .main-wrap
    include ../sections/header

    //begin content
    main.main.my-course
      include ../sections/profile-header
      .container.my-course__header
        .my-course__header-left
          .my-course__header-label.my-course__header-label--psychology
            span Психология
          h2.my-course__title Курс Психология 2.0
        .my-course__return-btn
          a(href="#").btn.btn--light Мои курсы



      .my-course-main
        .container
          include ../sections/my-course-aside

          //begin my-course-body
          .my-course-body
            include ../sections/my-course-body-tabs


          //end profile-edit-body

    //end content

    include ../sections/footer
